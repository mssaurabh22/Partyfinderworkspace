<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <FrameLayout
        android:id="@+id/frameContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#232323"
        android:clickable="true" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"
            android:orientation="vertical"
            android:weightSum="1" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="@dimen/weight_header" >

                <RelativeLayout
                    android:id="@+id/header_layout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:orientation="horizontal" >

                    <RelativeLayout
                        android:id="@+id/backButton_layout"
                        android:layout_width="80dp"
                        android:layout_height="match_parent"
                        android:layout_alignParentLeft="true" >

                        <ImageButton
                            android:id="@+id/back_img"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/back_img_left_margin"
                            android:background="#00000000"
                            android:src="@drawable/addback" />
                    </RelativeLayout>

                    <com.partyfinder.CustomView.CustomFontSemiBold
                        android:id="@+id/ordersummary_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:text="@string/ordersummry"
                        android:textColor="@color/layout_top_heading_color"
                        android:textSize="@dimen/All_screen_header_text_size" />

                    <com.partyfinder.CustomView.CustomFontRegular
                        android:id="@+id/heder_line"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/order_history_divider_height"
                        android:layout_alignParentBottom="true"
                        android:background="@color/orderHistorydividercolor" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.43"
                android:orientation="vertical"
                android:weightSum="1" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.4"
                    android:gravity="center_vertical" >

                    <LinearLayout
                        android:id="@+id/orsummary_headerbelow_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/orsummary_headerbelow_layout_left_margin"
                        android:gravity="center_vertical"
                        android:orientation="vertical" >

                        <com.partyfinder.CustomView.CustomFontRegular
                            android:id="@+id/ordSumaryUserName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Rick D.Schnabel"
                            android:textColor="@color/text1_text_colour"
                            android:textSize="@dimen/orderTicketConfirmation_Username_textsize" />

                        <com.partyfinder.CustomView.CustomFontRegular
                            android:id="@+id/ordSumryCity"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/text3_top_margin"
                            android:text="1506 Reynolds Alley"
                            android:textColor="@color/text2_text_colour"
                            android:textSize="@dimen/orderTicketConfirmation_Useradd_textsize" />

                        <com.partyfinder.CustomView.CustomFontRegular
                            android:id="@+id/ordSumryState"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/text2_top_margin"
                            android:text="1506 Reynolds Alley"
                            android:textColor="@color/text2_text_colour"
                            android:textSize="@dimen/orderTicketConfirmation_Useradd_textsize" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.02"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp" >
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.2"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp" >

                    <Button
                        android:id="@+id/ordSumryUseCurrentCard"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:background="@color/orsummary_btn_backround_colour"
                        android:text="@string/useCurrentCard"
                        android:textColor="@color/orsummary_btn_text_colour" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.07"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp" >
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.2"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp" >

                    <Button
                        android:id="@+id/ordSumryNewCard"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:background="@color/orsummary_btn_backround_colour"
                        android:text="@string/AddnewCard"
                        android:textColor="@color/orsummary_btn_text_colour" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.08" >

                <LinearLayout
                    android:id="@+id/edit_layout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/orsummarybelow_edittext_backround_colour"
                    android:orientation="vertical"
                    android:padding="5dp" >

                    <com.partyfinder.CustomView.CustomFontRegular
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/orsummarybelow_edittext_backround_colour"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/orsummarybelow_edittext_leftpadding"
                        android:text="Sua Ordem"
                        android:textColor="@color/orsummarybelow_edittext_text_colour"
                        android:textSize="@dimen/orsummarybelow_edittext_text_size" />
                    
                    <LinearLayout 
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">
                        
                        <RelativeLayout 
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical">

                    <com.partyfinder.CustomView.CustomFontRegular
                        android:id="@+id/TicketCommision"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@color/orsummarybelow_edittext_backround_colour"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/orsummarybelow_edittext_leftpadding"
                        android:textColor="@color/orsummarybelow_edittext_text_colour"
                        android:textSize="@dimen/order_history_listView_item_text1_TextSize"
                        android:visibility="visible"
                        android:text="Taxa de Conveniência " />

                           <com.partyfinder.CustomView.CustomFontRegular
                               android:id="@+id/TicketCommisionCost"
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:layout_alignParentRight="true"
                               android:layout_alignParentTop="true"
                               android:background="@color/orsummarybelow_edittext_backround_colour"
                               android:gravity="center_vertical"
                               android:paddingRight="@dimen/orsummarybelow_edittext_leftpadding"
                               android:text="20"
                               android:textColor="@color/orsummarybelow_edittext_text_colour"
                               android:textSize="@dimen/order_history_listView_item_text1_TextSize"
                               android:visibility="gone" />
                           
                           </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.12" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#232323" >

                    <TextView
                        android:id="@+id/orderSummryEventname"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="@dimen/listview_layout_left_margin"
                        android:layout_marginRight="@dimen/listview_layout_right_margin"
                        android:layout_marginTop="@dimen/order_summery_total_price_margin"
                        android:text="Month End Party"
                        android:textColor="@color/listview_edittext1_text_colour"
                        android:textSize="@dimen/orderTicketConfirmation_ticketOrderCode_detailtextSize" />

                    <RelativeLayout
                        android:id="@+id/aaaa"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/orderSummryEventname"
                        android:layout_marginBottom="@dimen/order_summery_total_price_margin"
                        android:layout_marginTop="@dimen/order_summery_total_price_margin"
                        android:orientation="horizontal" >

                        <com.partyfinder.CustomView.CustomFontRegular
                            android:id="@+id/total"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/listview_layout_left_margin"
                            android:text="Total"
                            android:textColor="@color/listview_edittext2_text_colour"
                            android:textSize="@dimen/orderTicketConfirmation_ticketOrderCode_detailtextSize" />

                        <com.partyfinder.CustomView.CustomFontRegular
                            android:id="@+id/totaPrice"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="@dimen/listview_layout_right_margin"
                            android:textColor="#ffffff"
                            android:textSize="@dimen/order_history_listView_item_text_TextSize" />
                    </RelativeLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="0.8dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="13dp"
                        android:layout_marginRight="13dp"
                        android:background="@drawable/myprofiledivider" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.26" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/tcketLayout"
                    android:background="#232323"
                    android:orientation="vertical" >

                    <ListView
                        android:id="@+id/orderSumryList"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#232323"
                        android:divider="@drawable/myprofiledivider"
                        android:listSelector="#00000000"
                        android:overScrollMode="never"
                        android:paddingLeft="@dimen/order_history_listView_item_datetext_leftmargin"
                        android:paddingRight="@dimen/order_history_listView_item_datetext_leftmargin"
                        android:scrollbars="none"
                        tools:listitem="@layout/ordersummary_listview_layout" >
                    </ListView>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>

</RelativeLayout>