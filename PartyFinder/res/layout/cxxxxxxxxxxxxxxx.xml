<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#232323"
    android:orientation="vertical" >

    <FrameLayout
        android:id="@+id/frameContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true" >

        <RelativeLayout
            android:id="@+id/main_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#232323" >

            <RelativeLayout
                android:id="@+id/header_layout"
                android:layout_width="match_parent"
                android:layout_height="@dimen/bottam_tab_hight"
                android:background="#ffffff" >

                      <RelativeLayout 
                          android:id="@+id/backButton_layout"
                    android:layout_height="match_parent"
                    android:layout_width="80dp"
                    android:layout_alignParentLeft="true">
                <ImageButton
                    android:id="@+id/addtcket_back_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/order_history_header_image_leftmargin"
                    android:background="#ffffff"
                    android:gravity="center"
                    android:src="@drawable/addback" />
                <!-- android:layout_marginBottom="16px" -->
   </RelativeLayout>
                <com.partyfinder.CustomView.CustomFontSemiBold
                    android:id="@+id/header"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="@string/addticket"
                    android:textColor="@color/layout_top_heading_color"
                    android:textSize="@dimen/All_screen_header_text_size"
                    android:textStyle="bold" />
                
                <RelativeLayout 
                     android:id="@+id/forwardButton_layout"
                    android:layout_height="match_parent"
                    android:layout_width="80dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:layout_alignParentRight="true">

                <ImageButton
                    android:id="@+id/addticket_forward_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                 
                    android:layout_marginBottom="16px"
                    android:layout_marginRight="@dimen/order_history_header_image_leftmargin"
                    android:background="#ffffff"
                    android:src="@drawable/forward" />
                
                </RelativeLayout>
            </RelativeLayout>

            <ListView
                android:id="@+id/addticket_listview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/header_layout"
             
                android:overScrollMode="never"
                android:background="#232323"
                android:scrollbars="none"
                tools:listitem="@layout/addticketslistdetail" >
            </ListView>

            <ProgressBar
                android:id="@+id/addTicketProg"
                android:layout_width="@dimen/hdpi_progressBar_height"
                android:layout_height="@dimen/hdpi_progressBar_height"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:visibility="gone" />
        </RelativeLayout>
    </FrameLayout>

</LinearLayout>